{
  "name": "kinnect",
  "version": "1.0.0",
  "description": "Remotely control robotic instruments with MIDI over web",
  "main": "index.html",
  "scripts": {
    "start": "nodemon server",
    "dev": "parcel client/public/index.html",
    "build": "parcel build client/public/index.html",
    "test": "jest --coverage",
    "format": "prettier --write \"client/**/*.{js,jsx}\"",
    "prettier-watch": "onchange \"client/**/*.{js,jsx}\" -- prettier --write --ignore-unknown {{changed}}",
    "lint": "eslint \"src/**/*.{js,jsx,tsx}\" --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlstarcher/kinnect.git"
  },
  "keywords": [
    "Raspberry",
    "Pi"
  ],
  "author": "Matthew Starcher",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mlstarcher/kinnect/issues"
  },
  "homepage": "https://github.com/mlstarcher/kinnect#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nanotimer": "^0.3.15",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "simple-peer": "^9.11.0",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.0.0",
    "step-sequencer": "^0.1.1",
    "time-stamp": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "babel-eslint": "^10.1.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.0.4",
    "morgan": "^1.10.0",
    "onchange": "^7.1.0",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^2.2.1"
  }
}
